  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "start": "npm run develop",
    "clean": "rimraf .cache public",
    "build": "npm run clean && gatsby build",
    "develop": "npm run clean && gatsby develop",
    "serve": "gatsby serve",
    "serve.prod": "http-server -p 4000 ./public",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier:check": "prettier --config ./.prettierrc --list-different \"src/**/*{.ts,.js,.json,.css,.scss}\"",
    "prettier:fix": "prettier --config ./.prettierrc --staged --write \"src/**/*\"",
    "prettier:quick": "pretty-quick --stage"
  },
  "devDependencies": {
    "http-server": "^0.12.1",
    "prettier": "^1.19.1",
    "rimraf": "^2.7.1"
  }
